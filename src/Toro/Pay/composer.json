{
    "name": "toro/pay-php",
    "type": "library",
    "description": "Toro Pay API client in PHP.",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "doctrine/collections": "^1.5",
        "doctrine/inflector": "^1.2",
        "league/oauth2-client": "^2.2",
        "symfony/options-resolver": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "sylius/user": "^1.0",
        "symfony/security": "^3.4",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Toro\\Pay\\": "src/",
            "Tests\\Toro\\Pay\\": "tests/"
        }
    },
    "autoload-dev": {
        "files": [ "src/dump.php" ]
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}
